{% for i in range(0, dict[1]|length) %}
<div class="review">
<div class="review-left">
<div class="reviewer">
<a href={{dict[1][i]["reviewer_url"]}}>
{{dict[1][i]["reviewer_name"]}}
</a>
</div>
<div class="review-rating">

<span style="padding-left: 10px">{{dict[1][i]["review_title"]}}</span>
</div>
<div class="review-date">Reviewed in the United States ðŸ‡ºðŸ‡¸ on February 13, 2023</div>
<div class="review-text">
<p>{{dict[1][i]["text"]}}</p>
</div>
</div>
<div class="review-right" style="">
{% if dict[1][i]["is_real"] == True %}
<i class="fa fa-check-circle" aria-hidden="true"></i>
{% else %}
<i class="fa fa-times-circle" aria-hidden="true"></i>
{% endif %}
</div>
</div>
{% endfor %}
<div class="item-count-display">
<a class="previous page-button">&laquo; Previous</a>
<span>Showing 1-2 of {{dict[1] | length}} reviews</span>
<a class="next page-button">Next &raquo;</a>

</div>
